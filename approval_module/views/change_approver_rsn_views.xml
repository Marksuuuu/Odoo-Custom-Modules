<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="change_approver_rsn_tree" model="ir.ui.view">
        <field name="name">change.approver.rsn.tree</field>
        <field name="model">change.approver.rsn</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="approval_type"/>
                <field name="date"/>
            </tree>
        </field>
    </record>

    <record id="action_view_change_history" model="ir.actions.act_window">
        <field name="name">Reason for Change History</field>
        <field name="res_model">change.approver.rsn</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Generate Customer Details
            </p>
        </field>
    </record>
</odoo>