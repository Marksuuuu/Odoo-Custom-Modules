<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="service_type_form_view" model="ir.ui.view">
            <field name="name">service_type_form_view</field>
            <field name="model">service.type</field>
            <field name="arch" type="xml">
                <form string="Service Type From">
                    <sheet>
                        <div class="oe_title">
                            <label for="name" string="Control No." class="oe_edit_only"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="service_type_tree_view" model="ir.ui.view">
            <field name="name">service_type_tree_view</field>
            <field name="model">service.type</field>
            <field name="arch" type="xml">
                <tree string="Tree" editable='bottom'>
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
